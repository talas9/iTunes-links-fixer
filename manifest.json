{
  "manifest_version": 2,
  "name": "iTunes links fixer",
  "short_name ": "iTunes links fixer",
  "version": "0.4",
  "content_scripts": [
    {
      "matches": [
        "https://itunes.apple.com/en/app/chrome/*","https://itunes.apple.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": { 
   "16": "icon16.png",
   "48": "icon48.png",
   "128": "icon128.png"
  },
  "permissions": [
   "activeTab"
   ]
}